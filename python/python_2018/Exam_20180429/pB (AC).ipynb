   "source": [
    "number = int(input())\n",
    "real_number = number\n",
    "factor = 2\n",
    "factor_ed = []\n",
    "\n",
    "while factor <= number:\n",
    "    if number % factor == 0:\n",
    "        number /= factor\n",
    "        factor_ed.append(factor) \n",
    "        continue\n",
    "    factor += 1\n",
    "    \n",
    "print(real_number, \"=\", end = \" \")\n",
    "\n",
    "print(*factor_ed, sep='*')\n"
   ]
